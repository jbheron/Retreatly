<%= bootstrap_form_for @venue, url: event_venues_path(Event.find(params[:event_id])), :html => {:class => "form-horizontal"} do |f| %>
  <%= f.text_field :name %>
  <%= f.text_field :address %>
  <%= f.text_field :city %>
  <%= f.text_field :country %>
  <%= f.label :lodging %>
  <%= f.text_area :lodging %>
  <%= f.label :travel %>
  <%= f.text_area :travel %>
  <%= f.submit %>
<% end %>