<%= form_for @event, url: events_path do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br><br>

  <%= f.label :price %>
  <%= f.text_field :price %><br><br>

  <%= f.label :description %>
  <%= f.text_area :description %><br><br>

  <%= f.label :start_date %>
  <%= f.date_field :start_date %><br><br>

  <%= f.label :end_date %>
  <%= f.date_field :end_date %><br><br>

  <%= f.submit %>
<% end %>